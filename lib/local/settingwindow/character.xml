<?xml version="1.0" encoding="UTF-8"?>
<ui_setting sep="|" order="general|window|output">
	<general sep="|" order="name|reply_character_format|reply_user_format|wait_time|echo|teachable">
		<name>
			<type>"Str"</type>
		</name>
		<reply_character_format>
			<type>"Str"</type>
		</reply_character_format>
		<reply_user_format>
			<type>"Str"</type>
		</reply_user_format>
		<wait_time>
			<type>"Int"</type>
			<range>(100,1000000)</range>
			<suffix>"ms"</suffix>
		</wait_time>
		<echo>
			<type>"Bool"</type>
		</echo>
		<teachable>
			<type>"Bool"</type>
		</teachable>
	</general>
	<window sep="|" order="window_size|window_pos|lock|auto_hide|nomral_transparency|mouse_on_transparency">
		<window_size>
			<type>"Size"</type>
			<w_range>(1,$SCREEN_WIDTH)</w_range>
			<h_range>(1,$SCREEN_HEIGHT)</h_range>
		</window_size>
		<window_pos>
			<type>"Pos"</type>
			<x_range>(-$SCREEN_WIDTH,$SCREEN_WIDTH)</x_range>
			<y_range>(-$SCREEN_HEIGHT,$SCREEN_HEIGHT)</y_range> 
		</window_pos>
		<lock>
			<type>"Bool"</type>
		</lock>
		<nomral_transparency>
			<type>"Int"</type>
			<range>(0,100)</range>
			<suffix>"%"</suffix>
		</nomral_transparency>
		<mouse_on_transparency>
			<type>"Int"</type>
			<range>(0,100)</range>
			<suffix>"%"</suffix>
		</mouse_on_transparency>
		<auto_hide>
			<type>"Bool"</type>
		</auto_hide>
	</window>
	<output sep="|" order="pos1|pos2|window_size|stay_time|nomral_transparency|mouse_on_transparency|auto_hide|close_when_click|string_pos|string_color|font|lock|boxes_number">
		<pos1>
			<type>"Pos"</type>
			<x_range>(-$SCREEN_WIDTH,$SCREEN_WIDTH)</x_range>
			<y_range>(-$SCREEN_HEIGHT,$SCREEN_HEIGHT)</y_range> 	
		</pos1>
		<pos2>
			<type>"Pos"</type>
			<x_range>(-$SCREEN_WIDTH,$SCREEN_WIDTH)</x_range>
			<y_range>(-$SCREEN_HEIGHT,$SCREEN_HEIGHT)</y_range> 
		</pos2>
		<window_size>
			<type>"Size"</type>
			<w_range>(1,$SCREEN_WIDTH)</w_range>
			<h_range>(1,$SCREEN_HEIGHT)</h_range>
		</window_size>
		<stay_time>
			<type>"Int"</type>
			<range>(100,100000)</range>
			<suffix>"ms"</suffix>
		</stay_time>
		<nomral_transparency>
			<type>"Int"</type>
			<range>(0,100)</range>
			<suffix>"%"</suffix>
		</nomral_transparency>
		<mouse_on_transparency>
			<type>"Int"</type>
			<range>(0,100)</range>
			<suffix>"%"</suffix>
		</mouse_on_transparency>
		<auto_hide>
			<type>"Bool"</type>
		</auto_hide>
		<close_when_click>
			<type>"Bool"</type>
		</close_when_click>
		<string_pos>
			<type>"Pos"</type>
			<x_range>(-$SCREEN_WIDTH,$SCREEN_WIDTH)</x_range>
			<y_range>(-$SCREEN_HEIGHT,$SCREEN_HEIGHT)</y_range> 
		</string_pos>
		<string_color>
			<type>"Color"</type>
		</string_color>
		<font>
			<type>"Font"</type>
		</font>
		<lock>
			<type>"Bool"</type>
		</lock>
		<boxes_number>
			<type>"Int"</type>
			<range>(0,100000)</range>
			<suffix>""</suffix>
		</boxes_number>
	</output>
</ui_setting>